{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.Hosting.Diagnostics": "Information"
    }
  },
  "AllowedHosts": "*",
  "CorsSettings": {
    "PolicyName": "AllowBlazorWasm",
    "AllowedOrigins": [
      "https://localhost:7072",
      "http://localhost:5122"
    ],
    "AllowAnyHeader": true,
    "AllowAnyMethod": true,
    "ExposedHeaders": [
      "Content-Disposition"
    ]
  },
  "Kestrel": {
    "Limits": {
      "MaxRequestBodySize": 20971520
    }
  }
}
